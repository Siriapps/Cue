# ===========================================
# CUE - Environment Configuration Template
# ===========================================
# Copy this file to .env and fill in your values

# API Keys
GEMINI_API_KEY=your_gemini_api_key_here
VEO_API_KEY=your_veo_api_key_here
GEMINI_MODEL=gemini-3-pro-preview
# Image generation for session card thumbnails (same API key as above)
GEMINI_IMAGE_MODEL=nano-banana-pro-preview

# Database
MONGODB_URI=mongodb+srv://user:password@cluster.mongodb.net/dbname

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# Supabase (optional)
SUPABASE_URL=
SUPABASE_ANON_KEY=

# Service URLs (change for production)
API_BASE_URL=http://localhost:8000
WS_BASE_URL=ws://localhost:8000
LIBRARY_URL=http://localhost:3001
